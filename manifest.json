{
    "manifest_version": 3,
    "name": "Login POST Capture",
    "version": "1.1",
  "icons": {
    "16": "icons/i.png",
    "48": "icons/i.png",
    "128": "icons/i.png"
  },
    "permissions": [
      "webRequest",
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  